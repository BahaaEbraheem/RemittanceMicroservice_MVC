      "Default": {
        "HostName": "localhost"
      }
    },
    "EventBus": {
      "ClientName": "MsDemo_IdentityService",
      "ExchangeName": "MsDemo"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
